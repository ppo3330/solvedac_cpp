//O(N^2 * M)

#include <iostream>
#include <vector>
#include <algorithm>
#include <queue>

using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	int N, M;
	cin >> N >> M;

	vector<vector<int>> arr;
	vector<vector<int>> dp;
	arr.assign(N, vector<int>(N));
	dp.assign(N, vector<int>(N));

	for (int i = 0; i < N; i++) {
		for (int j = 0; j < N; j++) {
			cin >> arr[i][j];
			if (i == 0 and j == 0) dp[i][j] = arr[i][j];
			else if (i == 0) dp[i][j] = dp[i][j - 1] + arr[i][j];
			else if (j == 0) dp[i][j] = dp[i - 1][j] + arr[i][j];
			else dp[i][j] = dp[i - 1][j] + dp[i][j - 1] - dp[i-1][j-1] + arr[i][j];
		}
	}

	vector<int> result;
	for (int i = 0; i < M; i++) {
		int x1, x2, y1, y2;
		cin >> x1 >> y1 >> x2 >> y2;

		if (x1 == 1 and y1 == 1) result.push_back(dp[x2 - 1][y2 - 1]);
		else if (x1 == 1) result.push_back(dp[x2 - 1][y2 - 1] - dp[x2 - 1][y1 - 2]);
		else if (y1 == 1) result.push_back(dp[x2 - 1][y2 - 1] - dp[x1 - 2][y2 - 1]);
		else result.push_back(dp[x2 - 1][y2 - 1] - dp[x2 - 1][y1 - 2] - dp[x1 - 2][y2 - 1] + dp[x1 - 2][y1 - 2]);
	}

	for (int i = 0; i < M; i++) {
		cout << result[i] << "\n";
	}
}
