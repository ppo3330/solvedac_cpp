//O(N log M)
//M은 값의 최댓값 -> log M은 (한 수 당 배수를 이용한 탐색의 평균)
//에라토스 테네스의 체
//각 소수의 배수를 제거해 소수 판별
#include <iostream>
#include <vector>

using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int N;
	cin >> N;
	vector<int> arr(N);
	vector<int> numbers(1000001, 0);
	vector<int> results(1000001, 0);

	for (int i = 0; i < N; i++) {
		cin >> arr[i];
		numbers[arr[i]] = 1;
	}
	
	for (int i = 0; i < N; i++) {
		for (int j = arr[i]*2; j < 1000001; j += arr[i]) {
			if (numbers[j] == 1) {
				results[arr[i]]++;
				results[j]--;
			}
		}
	}

	for (int i = 0; i < N; i++) {
		cout << results[arr[i]]<<" ";
	}

}
