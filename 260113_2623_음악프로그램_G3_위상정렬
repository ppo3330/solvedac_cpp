//O(N + E) E: 전체 간선 수 -> O(M*N)
//위상정렬 기억 잘해서 풀었다.
#include <iostream>
#include <vector>
#include <deque>

using namespace std;

int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int N, M;
	cin >> N >> M;

	vector<vector<int>> nxts(N + 1);
	vector<int> indegree(N + 1, 0);
	vector<int> result;

	for (int i = 0; i < M; i++) 
	{
		int n, pre;
		cin >> n;
		if (n == 1) continue;
		cin >> pre;
		for (int j = 0; j < n-1; j++) 
		{
			int nxt;
			cin >> nxt;
			nxts[pre].push_back(nxt);
			indegree[nxt]++;
			pre = nxt;
		}
	}

	deque<int> dq;

	for (int i = 1; i <= N; i++) {
		if (indegree[i] == 0)
		{
			dq.push_back(i);
		}
	}

	while (!dq.empty()) {
		int cur = dq.front();
		dq.pop_front();
		result.push_back(cur);

		for (int i = 0; i < nxts[cur].size(); i++)
		{
			indegree[nxts[cur][i]]--;
			if (indegree[nxts[cur][i]] == 0)
			{
				dq.push_back(nxts[cur][i]);
			}
		}
	}
	if (result.size() != N)
		cout << 0;
	else {
		for (int i = 0; i < result.size(); i++) {
			cout << result[i] << "\n";
		}
	}
}
