//O((RC + (R+C))* T )
//그러나 O( RC * T)로 표기해도 됨
#include <iostream>
#include <vector>
#include <queue>

using namespace std;

int R, C, T;
vector<vector<int>> arr;
int upair = -1, botair;
int dirr[] = { 0,0,1,-1 };
int dirc[] = { 1,-1,0,0 };

void diff() {
	vector<vector<int>> visited(R, vector<int>(C, 0));

	for (int i = 0; i < R; i++) {
		for (int j = 0; j < C; j++) {
			if (arr[i][j] <= 0) continue;

			for (int k = 0; k < 4; k++) {
				int forr = dirr[k] + i;
				int forc = dirc[k] + j;
				if (forr < 0 or forr >= R or forc < 0 or forc >= C) continue;
				if (arr[forr][forc] == -1) continue;

				visited[forr][forc] += arr[i][j] / 5;
				visited[i][j] -= arr[i][j] / 5;
			}
		}
	}

	for (int i = 0; i < R; i++) {
		for (int j = 0; j < C; j++) {
			arr[i][j] += visited[i][j];
		}
	}
}

void rotate() {

	for (int i = upair-1; i > 0; i--) arr[i][0] = arr[i - 1][0];
	for (int j = 0; j < C - 1; j++) arr[0][j] = arr[0][j + 1];
	for (int i = 0; i < upair; i++) arr[i][C-1] = arr[i+1][C-1];
	for (int j = C - 1; j > 1; j--) arr[upair][j] = arr[upair][j - 1];

	for (int i = botair +1; i < R - 1; i++) arr[i][0] = arr[i + 1][0];
	for (int j = 0; j < C - 1; j++) arr[R-1][j] = arr[R-1][j + 1];
	for (int i = R - 1; i > botair; i--) arr[i][C - 1] = arr[i - 1][C - 1];
	for (int j = C - 1; j > 1; j--) arr[botair][j] = arr[botair][j - 1];

	arr[upair][1] = 0;
	arr[botair][1] = 0;
}

int result() {
	int res = 0;
	for (int i = 0; i < R; i++) {
		for (int j = 0; j < C; j++) {
			if (arr[i][j] > 0) res += arr[i][j];
		}
	}
	return res;
}

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	cin >> R >> C >> T;
	arr.assign(R, vector<int>(C, 0));

	for (int i = 0; i < R; i++) {
		for (int j = 0; j < C; j++) {
			cin >> arr[i][j];
			if (arr[i][j] == -1) {
				if (upair == -1) upair = i;
				else botair = i;
			}
		}
	}

	for (int t = 0; t < T; t++) {
		diff();
		rotate();
	}
	
	cout << result();
}	
